    <h4 class="mb-4 text-muted">Integrated Systems</h4>
    <div class="row">
        <div class="col-md-3 col-lg-2 mb-4 align-middle">
            <span>{{ svg-jar "arcGis"}}</span>
            <h4 class="mt-4 text-muted">ARCGIS MAPPING SYSTEM</h4>
            <LinkTo @route="dash" @disabled={{true}}>
                <a href="#" class="bg-green btn btn-lg">view</a>
            </LinkTo>
        </div>
        <div class="col-md-3 col-lg-2 mb-4 align-middle">
            <span>{{ svg-jar "county_revenue"}}</span>
