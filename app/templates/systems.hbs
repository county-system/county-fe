            <LinkTo @route="dash" @disabled={{true}}>
                <a href="#" class="bg-green btn btn-lg">view</a>
            </LinkTo>
        </div>
        <div class="col-md-3 col-lg-2 mb-4">
            <span>{{ svg-jar "partner_donor"}}</span>
            <h4 class="mt-3 text-muted">{{uppercase "Partner and Donor Fund Systems"}}</h4>
            <LinkTo @route="dash" @disabled={{true}}>
                <a href="#" class="bg-green btn btn-lg">view</a>
            </LinkTo>
        </div>
        <div class="col-md-3 col-lg-2 mb-4">
            <span>{{ svg-jar "kobo"}}</span>
            <h4 class="mt-5 text-muted">{{uppercase "kobo toolbox"}}</h4>
            <LinkTo @route="dash" @disabled={{true}}>
                <a href="#" class="bg-green btn btn-lg">view</a>
            </LinkTo>
        </div>
    </div>
